{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://maksimsolo.github.io/TODOLIST",
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.3",
    "@mui/lab": "^6.0.0-beta.11",
    "@mui/material": "^6.1.3",
    "@mui/styles": "^6.1.3",
    "@reduxjs/toolkit": "^2.2.8",
    "@storybook/addon-storysource": "^8.3.5",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/react-redux": "^7.1.34",
    "axios": "^1.7.7",
    "formik": "^2.4.6",
    "gh-pages": "^6.2.0",
    "jest-image-snapshot": "^6.4.0",
    "jest-puppeteer": "^10.1.2",
    "puppeteer": "^23.5.3",
    "re-reselect": "^5.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.27.0",
    "react-scripts": "^5.0.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "start-server-and-test": "^2.0.8",
    "uuid": "^10.0.0",
    "web-vitals": "^4.2.3"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public --no-dll",
    "build-storybook": "build-storybook -s public",
    "jest:integration": "jest -c integration/jest.config.js",
    "test:integration": "start-server-and-test storybook http-get://localhost:9009 jest:integration"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.25.8",
    "@babel/plugin-transform-runtime": "^7.25.7",
    "@storybook/addon-actions": "^8.3.5",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.3.5",
    "@storybook/addon-links": "^8.3.5",
    "@storybook/node-logger": "^8.3.5",
    "@storybook/preset-create-react-app": "^8.3.5",
    "@storybook/react": "^8.3.5",
    "@storybook/testing-library": "^0.2.2",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "typescript": "^4.9.5"
  }
}
